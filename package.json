{
    "name": "sej",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/andrewisen-tikab/sej"
    },
    "version": "0.0.50",
    "type": "module",
    "main": "dist/sej.js",
    "author": "André Wisén",
    "license": "MIT",
    "scripts": {
        "dev": "vite",
        "preview": "vite preview",
        "build": "tsc && vite build && vite build -c vite.config.example.ts",
        "ts": "tsc",
        "lint": "eslint --ext .ts src",
        "docs": "typedoc",
        "test": "cd test && yarn run cypress run --browser chrome",
        "cruise": "yarn cruise:1 && yarn cruise:2 && yarn cruise:3",
        "cruise:1": "npx depcruise src --include-only '^src' --config --collapse 2 --output-type dot | dot -T svg > ./resources/dependency-graph-01.svg",
        "cruise:2": "npx depcruise src --include-only '^src' --config --collapse 3 --output-type dot | dot -T svg > ./resources/dependency-graph-02.svg",
        "cruise:3": "npx depcruise src --include-only '^src' --config --collapse 4 --output-type dot | dot -T svg > ./resources/dependency-graph-03.svg"
    },
    "devDependencies": {
        "@trivago/prettier-plugin-sort-imports": "^4.2.0",
        "@types/proj4": "^2.5.4",
        "@types/signals": "^1.0.3",
        "@types/three": "^0.166.0",
        "@typescript-eslint/eslint-plugin": "^6.4.0",
        "dependency-cruiser": "^14.1.2",
        "eslint": "^8.0.1",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-typescript": "^17.1.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-config-standard-with-typescript": "latest",
        "eslint-plugin-import": "^2.28.1",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
        "eslint-plugin-prefer-arrow-functions": "^3.1.4",
        "eslint-plugin-promise": "^6.0.0",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "prettier": "^3.0.3",
        "typedoc": "^0.25.2",
        "typescript": "*",
        "vite": "^4.5.3",
        "vite-plugin-dts": "^3.6.0",
        "vite-plugin-top-level-await": "^1.3.1"
    },
    "dependencies": {
        "3d-tiles-renderer": "^0.3.35",
        "@andrewisen/error-manager": "^0.0.4",
        "camera-controls": "^2.8.5",
        "lil-gui": "^0.18.2",
        "localforage": "^1.10.0",
        "proj4": "^2.9.1",
        "signals": "^1.0.0",
        "three": "^0.166.1",
        "three-mesh-bvh": "^0.7.6",
        "three-multi-select": "^0.0.23",
        "three-shape-3d": "^0.0.26",
        "three-spatial-hash-grid": "^0.0.5",
        "three-timeline": "^0.0.2",
        "zod": "^3.23.8"
    },
    "packageManager": "yarn@4.3.1"
}
